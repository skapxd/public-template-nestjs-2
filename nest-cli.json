{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "watchAssets": true,
    "assets": [
      {"include": "**/*.*", "outDir": "dist"}
    ],
    "plugins": [
      "@nestjs/swagger"
    ],
    "builder": "swc",
    "typeCheck": false
  }
}